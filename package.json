{
  "name": "ethspring",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn prepare-ethereum-org-website && docusaurus start",
    "build": "yarn prepare-ethereum-org-website && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "// 1": "Prepares the ethereum-org-website code so we can import components from it",
    "prepare-ethereum-org-website": "cd node_modules/ethereum-org-website && yarn merge-translations"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-alpha.72",
    "@docusaurus/preset-classic": "2.0.0-alpha.72",
    "@mdx-js/react": "^1.6.21",
    "clsx": "^1.1.1",
    "ethereum-org-website": "https://github.com/ethereum/ethereum-org-website#v2.27.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rehype-katex": "^4.0.0",
    "remark-math": "^3.0.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0"
  }
}
